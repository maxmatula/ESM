@{
    ViewBag.Title = "Upload file";
}

<h2>Dodaj plik</h2>


    @using (Html.BeginForm("UploadFile", "Files", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div>
            @Html.TextBox("file", "", new { type = "file" }) <br />

            <input type="submit" value="Upload" />

            @ViewBag.Message
        </div>
    }
